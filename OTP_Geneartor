// 4 Digit OTP Generator

function OTP(){
    const min = 1000
    const max = 9999
    return (Math.floor(Math.random()*(max-min+1)+min))
}

console.log(OTP());


// 6 Digit OTP Generator

function OTP2(){
    const min = 100000
    const max = 999999
    return (Math.floor(Math.random()*(max-min+1)+min))
}

console.log(OTP2());
