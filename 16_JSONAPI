// Object destructure
// Object destructuring in js is a clean way to extract values from an object into variables
// variables names must match the object keys

const course = {
    coursename: "js in hindi",
    price: "999",
    courseInstructor: "hitesh"
}

// course.courseInstructor  

const {courseInstructor} = course   // hamein kahan se value extract krni h, matlb kis object se value extract krni hai

console.log(courseInstructor)

// Renaming variables
// const {courseInstructor: instructor} = course
// console.log(instructor)


